<% @order.line_items.each do |item|%>

<tr>
  <td>
  <%= image_tag item.product.image.tiny %>
  </td>
  <td>
    <h4>
      <%=item.product.name%>
    </h4>
    <br>
    <p>
      <%= item.product.description %>
    </p>
  </td>
  <td> 
  $<%= item.product.price / item.product.quantity %>
  </td>

  <td>
    <%= item.product.quantity %>
  </td>

  <td>
    $<%= item.product.price %>
  </td>
</tr>
<%end%>